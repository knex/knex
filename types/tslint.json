{
  "extends": "dtslint/dtslint.json",
  "rules": {
    "ban-types": false,
    "await-promise": false,
    "array-type": false,
    "no-unnecessary-generics": false,
    "no-empty-interface": false,
    "unified-signatures": false,
    "no-unnecessary-qualifier": false,
    "strict-export-declare-modifiers": false
  }
}
